# editorconfig.org
root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.{js,scss,html,xml}]
indent_style = tab
indent_size = 4

[*.java]
indent_style = space
indent_size = 4

[Jenkinsfile]
indent_style = space
indent_size = 4

[*.{yml,json}]
indent_style = space
indent_size = 2
